{
  "name": "@aragon/govern-subgraph",
  "version": "0.1.2",
  "scripts": {
    "codegen": "rm -rf generated; graph codegen",
    "build": "rm -rf build; graph build",
    "manifest-mainnet": "scripts/build-manifest.sh mainnet",
    "manifest-mainnet-staging": "env STAGING=true scripts/build-manifest.sh mainnet",
    "manifest-rinkeby": "scripts/build-manifest.sh rinkeby",
    "manifest-rinkeby-staging": "env STAGING=true scripts/build-manifest.sh rinkeby",
    "manifest-xdai": "env scripts/build-manifest.sh xdai",
    "manifest-xdai-staging": "env STAGING=true scripts/build-manifest.sh xdai",
    "deploy-mainnet": "scripts/deploy.sh 1hive conviction-voting mainnet",
    "deploy-mainnet-staging": "env STAGING=true scripts/deploy.sh 1hive conviction-voting mainnet",
    "deploy-rinkeby": "scripts/deploy.sh 1hive conviction-voting rinkeby",
    "deploy-rinkeby-staging": "STAGING=true scripts/deploy.sh evalir eg rinkeby",
    "deploy-xdai": "scripts/deploy.sh 1hive conviction-voting xdai",
    "deploy-xdai-staging": "STAGING=true scripts/deploy.sh 1hive c-v xdai"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1",
    "mustache": "^4.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0"
  }
}
