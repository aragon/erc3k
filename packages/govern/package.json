{
  "name": "@aragon/govern",
  "version": "1.0.1-beta.0",
  "description": "Govern JS Client",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "main": "dist/cjs/public/index.js",
  "module": "dist/esm/public/index.js",
  "types": "dist/cjs/public/index.d.ts",
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "dev:esm": "yarn build:esm --watch",
    "dev:cjs": "yarn build:cjs --watch",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aragon/govern.git"
  },
  "homepage": "https://github.com/aragon/govern",
  "keywords": [
    "Ethereum",
    "JavaScript",
    "API",
    "Govern",
    "Optimistic Voting"
  ],
  "authors": [
    {
      "name": "Samuel Furter",
      "homepage": "https://github.com/nivida"
    }
  ],
  "devDependencies": {
    "@ethereumjs/config-tsc": "^1.1.1",
    "@ethereumjs/config-tslint": "^1.1.1",
    "@types/jest": "^26.0.15",
    "coveralls": "^3.1.0",
    "jest": "^26.6.1",
    "ts-jest": "^26.4.2",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@urql/core": "^1.13.1",
    "graphql-tag": "^2.11.0",
    "isomorphic-unfetch": "^3.1.0"
  }
}
